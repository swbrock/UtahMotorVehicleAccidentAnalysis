@{
    Layout = "_Layout1"
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Counties" asp-route-county=""
   class="btn @(ViewBag.SelectedType == null ? "btn-primary" : "btn-outline-secondary") col-md justify-content-center">All</a>

@foreach (string a in Model)
{
    <a asp-controller="Home" asp-action="Counties" asp-route-county="@a"
       class="btn @(ViewBag.SelectedType == a ? "btn-primary" : "btn-outline-secondary") col-md justify-content-center">@a</a>
}
